    "import sys\n",
    "num = int(input(\"What number's factorial do you want to find: \"))\n",
    "def factorialf(x):\n",
    "    f = 1\n",
    "    for i in range(2, x + 1):\n",
    "            f = f*i\n",
    "    return f\n",
    "print(\"The fiactorial of\", num, \"is:\",factorialf(num))"
